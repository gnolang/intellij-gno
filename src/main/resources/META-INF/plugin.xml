<idea-plugin>
  <id>com.github.intellij.gno</id>
  <name>Gno</name>
  <version>1.0</version>
  <vendor url="https://gno.land/">gnolang</vendor>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <fileType name="Gno File"
              implementationClass="com.github.intellij.gno.services.GnoFileType"
              extensions="gno"
              language="Gno"
    />
    <lang.syntaxHighlighterFactory language="Gno"
                                   implementationClass="com.github.intellij.gno.language.GnoSyntaxHighlighterFactory"
    />
    <annotator language="Gno" implementationClass="com.github.intellij.gno.language.GnoAnnotator"/>
    <lang.findUsagesProvider language="Gno"
                             implementationClass="com.github.intellij.gno.language.GnoFindUsagesProvider"/>
    <lang.foldingBuilder language="Gno" implementationClass="com.github.intellij.gno.language.GnoFoldingBuilder"/>
  </extensions>


  <applicationListeners>
  </applicationListeners>

</idea-plugin>
